<?xml version="1.0"?>

<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="WorkingDir" value="C:\Users\Administrador\Contractor"/>
    <!-- Corral args -->
    <add key="CorralDefaultArgs" value="/recursionBound:3" />
    
    <!-- CodeContracts args -->
    <add key="CccheckFullName" value="C:\Program Files (x86)\Microsoft\Contracts\Bin\cccheck.exe" />
    <add key="CccheckArgs" value="-stats=!! -remote -show=errors,validations -nobox -nologo -nopex -assemblyMode=standard -suggest=!! -warninglevel full -wp -adaptive -arithmetic -bounds -enum -nonnull -sortwarns=false" />
  </appSettings>
</configuration>